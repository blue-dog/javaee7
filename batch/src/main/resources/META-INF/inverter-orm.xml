            <basic name="iac" />
            <basic name="vac" />
            <basic name="fac" />
            <basic name="pac" />
            <basic name="etotal" />
            <basic name="htotal" />
            <basic name="mode" />
            <basic name="etoday" />
            <basic name="etotalh" />
            <basic name="htotalh" />
            <basic name="err_gv" />
            <basic name="err_gf" />
            <basic name="err_gz" />
            <basic name="err_temp" />
            <basic name="err_pv1" />
            <basic name="err_gfc1" />
            <basic name="err_mode" />
            <transient name="serialNumberInverter" />
        </attributes>
    </entity>
</entity-mappings>